{
  "expo": {
    "name": "Truffle-Shuffle-Native",
    "slug": "Truffle-Shuffle-Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "truffle-shuffle",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#000"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.ejhuang.truffle_shuffle"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ba940c50-88a2-4ad7-a917-c89d2b945ecb"
      }
    },
    "owner": "ehhuang.2015",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ba940c50-88a2-4ad7-a917-c89d2b945ecb"
    }
  }
}
